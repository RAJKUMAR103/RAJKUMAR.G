#include<stdio.h>
void main()
{
    int n;
    int *a1,*a2,*a3;
    printf("the items are \n1.mango,\n2.orange,\n3.tomatto");
    scanf("%d",&n);
    switch(n)
    {
        case 1:
        printf("%d",&a1);
        break;
        case 2:
        printf("%d",&a2);
        break;
        case 3:
        printf("%d",&a3);
        break;
        default:
        printf("no stacks available");
        
    }
}
